{
  "name": "promise-spy",
  "version": "1.0.0",
  "description": "Sets up an interceptor for Promise's which so we can get them later statically",
  "main": "lib/index",
  "module": "lib/index",
  "scripts": {
    "type-check": "tsc --noEmit",
    "type-check:watch": "npm run type-check -- --watch",
    "prepare": "tsc --emitDeclarationOnly && babel --out-dir lib --copy-files --extensions '.js,.ts' --source-maps inline src",
    "test": "mocha"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/MashupMill/promise-spy.git"
  },
  "keywords": [
    "promise",
    "spy",
    "flush",
    "wait",
    "async",
    "await",
    "intercept",
    "interceptor",
    "test"
  ],
  "author": "",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/MashupMill/promise-spy/issues"
  },
  "homepage": "https://github.com/MashupMill/promise-spy#readme",
  "devDependencies": {
    "@babel/cli": "^7.4.4",
    "@babel/core": "^7.4.5",
    "@babel/plugin-proposal-class-properties": "^7.4.4",
    "@babel/plugin-proposal-object-rest-spread": "^7.4.4",
    "@babel/preset-env": "^7.4.5",
    "@babel/preset-typescript": "^7.3.3",
    "@babel/register": "^7.4.4",
    "chai": "^4.2.0",
    "mocha": "^6.1.4",
    "sinon": "^7.3.2",
    "typescript": "^3.5.2"
  }
}
